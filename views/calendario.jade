extends layout

block custom-scripts  
  script(src='http://yui.yahooapis.com/3.10.3/build/yui/yui-min.js')
  script(src='javascripts/own/calendario.js')
block custom-stylesheets
  link(href='/stylesheets/own/shame.css', rel='stylesheet')
  link(href='/stylesheets/own/customCalendar.css', rel='stylesheet')

block content
    
  .container.text-center
    .span12.service-titles
      .span4
        hr
      .span4
        h3
          img(src='/images/iconoCaler.png', alt="Logo").pull-left
          span Capacitaciones
          img(src='/images/iconoCaler.png', alt="Logo").pull-right
      .span4
        hr

    .span12.columns-container
      .span4.pull-right
        #demo.yui3-skin-sam.yui3-g
          .yui3-u
            #mycalendar
            selected
              | date:
              span#selecteddate
                
      - var courses = [{ name: 'Formación de auditores internos', description: 'Formación de auditores internos, bajo la norma ISO 19011:2011 con enfoque en las normas ISO 15189:2007 e ISO/IEC 17025:2005' },{ name: 'Formación de auditores internos', description: 'Formación de auditores internos, bajo la norma ISO 19011:2011 con enfoque en las normas ISO 15189:2007 e ISO/IEC 17025:2005' },{ name: 'Formación de auditores internos', description: 'Formación de auditores internos, bajo la norma ISO 19011:2011 con enfoque en las normas ISO 15189:2007 e ISO/IEC 17025:2005' },{ name: 'Formación de auditores internos', description: 'Formación de auditores internos, bajo la norma ISO 19011:2011 con enfoque en las normas ISO 15189:2007 e ISO/IEC 17025:2005' },{ name: 'Formación de auditores internos', description: 'Formación de auditores internos, bajo la norma ISO 19011:2011 con enfoque en las normas ISO 15189:2007 e ISO/IEC 17025:2005' }];

      each course in courses
        .course
          include course-card


